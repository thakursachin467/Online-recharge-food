<div class="col-md-3 mt-4 ">
  <div class="menu  p-3 bg-light">
                <h5 class="text-primary">Your Order <span class="badge  badge-primary ">{{size sessions.cart}}</span></h5>
                <div class="nav flex-column">
                  <table class="table table-hover table-fixed">
                  <thead>
                  <tr>
                    <th style="width:50%">Product</th>
                    <th style="width:50%" class="text-center">Subtotal</th>

                  </tr>
                </thead>
                      {{#each sessions.cart}}
                              <tbody>
                    						<tr>
                    							<td data-th="Product">
                    								<div class="row">
                    									<div class="col-sm-10">
                    										<h6 class="nomargin">{{items.itemName}}</h6>
                                      <small class="badge badge-pill text-muted badge-light"> {{quantity}} &times; &#8377;{{items.itemPrice}} </small>
                    									</div>
                    								</div>
                    							</td>

                    							<td data-th="Subtotal" class="text-center">&#8377; {{totalPrice}}</td>
                    						</tr>
                    					</tbody>
                      {{/each}}
                      <tfoot>
                      <tr >
                        <td>Total:</td>
                        <td class="hidden-xs text-center align-middle"><strong> &#8377;{{total sessions.cart}}</strong></td>

                      </tr>
                    </tfoot>
                        </table>
                          {{#if (conditional1 (total sessions.cart) 0)}}
                        <tfoot>
              						<tr>
                            <td></td>
              							<td><a href="/cart" class="btn btn-success btn-block align-middle disabled">Continue</a></td>

              						</tr>
              					</tfoot>
                        {{else}}
                        <tfoot>
                          <tr>
                            <td></td>
                            <td><a href="/cart" class="btn btn-success btn-block align-middle">Continue</a></td>

                          </tr>
                        </tfoot>

                        {{/if}}
                </div>
            </div>

