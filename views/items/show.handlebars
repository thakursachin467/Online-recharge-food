  </div>
</div>

<div class="col-md-6">
  <!-- Website Overview -->
  <div class="tab-content">
      <div id="foodcourt" class="container tab-pane active"><br>
        <div class="panel panel-default">
<div class="panel-heading main-color-bg pl-3 pt-3 pb-3">
  <h3 ><i class="fa fa-cutlery" style="font-size:36px"></i> Food Court</h3>
</div>
<div class="panel-body mt-3">
  {{#each data}}

  {{#if (conditional ItemProvider "FOOD COURT")}}
      <div class=" card mt-2">
        <div class="row">
            <div class="col-md-9">
                <a href="/cart/{{id}}" class="list-group-item list-group-item-action">{{itemName}} <p>Price: {{itemPrice}}</p> <p> {{description}} </p>
                </a>
            </div>
            <div class="col-md-2">
              <a href="/cart/{{id}}" class="list-group-item list-group-item-action btn btn-outline-primary mt-4">Add</a>
            </div>
        </div>
      </div>
  {{/if}}


  {{/each}}
</div>
</div>
</div>
