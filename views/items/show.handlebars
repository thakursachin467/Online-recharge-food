<div class="panel-heading main-color-bg pl-3 pt-3 pb-3">
  <h3 ><i class="fa fa-cutlery" style="font-size:36px"></i> Food Court</h3>
</div>
<div class="panel-body mt-3">
  <table  class="table table-hover table-condensed mt-2">
    <thead>
    <tr>
      <th style="width:50%">Product</th>
      <th style="width:20%">Price</th>
      <th style="width:22%" class="text-center">ADD</th>
    </tr>
  </thead>
  {{#each data}}
  {{#if (conditional ItemProvider "FOOD COURT")}}
      <tbody>
        <tr>
          <td>
            <div class="row">
              <div class="col-sm-10">
                <h4 class="nomargin">{{itemName}}</h4>
                <p>{{description}}</p>
              </div>
            </div>
          </td>
          <td >&#8377; {{itemPrice}}</td>
          <td class="text-center">
              <a href="/cart/{{id}}"  class="btn btn-danger btn-sm"><i class="fa fa-cart-plus" style="font-size:16px"></i>Add</a>
          </td>
        </tr>
      </tbody>
  {{/if}}
