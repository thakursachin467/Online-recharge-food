
<div class="container "><br>
  <div class="panel-heading main-color-bg pl-3 pt-3 pb-3">
    <h3 ><i class="fa fa-cubes" style="font-size:36px"></i> All Orders</h3>
  </div>
  <div class="panel panel-default mt-3">
    <div class="panel-heading">

    </div>
    <div class="panel-body">
      <table id="test" class="table table-striped table-hover">
          <tr>
            <th>OrderId</th>
            <th>Amount Paid</th>
            <th>Order Date</th>
            <th>Details</th>

          </tr>


        <tbody>
          <tr>


{{#each data}}
          <tr>
            <td>{{OrderId}}</td>
            <td>{{conversion grandTotal}}</td>
            <td>{{formatdate date 'MMMM Do YYYY'}}</td>
            <td> <a href="/bill/{{OrderId}}" class="btn btn-primary">Click here</a> </td>
          </tr>

  {{/each}}

        </tbody>
        </table>
    </div>
  </div>

 <script type="text/javascript">
 var seen = {};
$('table tr').each(function() {
 var txt = $(this).text();
 if (seen[txt])
   $(this).remove();
 else
   seen[txt] = true;
});
</script>

</div>
